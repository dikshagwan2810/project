// Jenkinsfile

try {

  stage('checkout') {
      node {
        cleanWs()
        checkout scm
      }
  }
  stage('Build') {
    node {  
    	sh('echo "in Build PL now"')
      withCredentials([usernamePassword(credentialsId: 'unameandpassword', passwordVariable: 'GIT_PASSWORD', usernameVariable: 'GIT_USERNAME')]) {
    	sh('ls -la')
	sh('echo "This is a new text file" > new.txt')
	sh('git add .')
	sh('git commit -m "Adding txt file from jenkins"')
	sh('git status')
	sh('git config --global --unset http.proxy') 
	sh('git config --global --unset https.proxy')
 	sh('git push https://${GIT_USERNAME}:${GIT_PASSWORD}@github.com/dikshagwan2810/project.git HEAD:master')
	sh('ls -la')
	}
    }  
  } 
  stage('Deploy') {
     node {  
    	sh('echo "performing deployment"')
	}
    }
}
 
catch (org.jenkinsci.plugins.workflow.steps.FlowInterruptedException flowError) {
  currentBuild.result = 'ABORTED'
}
catch (err) {
  currentBuild.result = 'FAILURE'
  throw err
}
finally {
  if (currentBuild.result == 'SUCCESS') {
    currentBuild.result = 'SUCCESS'
  }
}
